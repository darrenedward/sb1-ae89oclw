{
    "name": "myrestaurant/silverstripe-restaurant",
    "description": "Restaurant Management System built with SilverStripe",
    "type": "silverstripe-project",
    "require": {
        "php": "^8.1",
        "silverstripe/cms": "^4.11",
        "silverstripe/framework": "^4.11",
        "silverstripe/admin": "^1.11",
        "silverstripe/asset-admin": "^1.11",
        "silverstripe/versioned": "^1.11",
        "silverstripe/session-manager": "^1.2",
        "symbiote/silverstripe-queuedjobs": "^4.9",
        "silverstripe/login-forms": "^4.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "silverstripe/developer-docs": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "process-timeout": 600
    },
    "extra": {
        "resources-dir": "_resources",
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ]
    }
}